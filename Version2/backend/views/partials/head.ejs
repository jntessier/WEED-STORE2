  <script id="install"> 
    function installapp()
    {
    
        if (promptDiffere) 
        {
            promptDiffere.prompt();
            promptDiffere.userChoice.then(function(choiceResult) 
            {
                if (choiceResult.outcome === 'dismissed') 
                {
                console.log('Installation cancellée');
                } 
                else 
                {
                console.log('Usager a installé notre application');
                }
            });
            promptDiffere = null;
        }
    }
</script>